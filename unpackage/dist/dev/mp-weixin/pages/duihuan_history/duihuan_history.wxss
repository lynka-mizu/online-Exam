
page {
	background-color: #FAFAFA;
}
.his-item {
	width: 96%;
	height: 180rpx;
	margin: 0 auto 20rpx;
	padding-top: 10rpx;
	padding-left: 15rpx;
	background-color: #FFFFFF;	
	border-radius: 15rpx;
}
.trade-no {
}
.goods-info {
	height: 60rpx;
	margin-bottom: 40rpx;
	display: flex;
	align-items: center;
	position: relative;
}
.goods-type {
	max-width: 62%;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
.shouli {
	margin-left: 10rpx;
	display: inline-block;
	padding: 4rpx;
	border: 2rpx solid #969696;
	border-radius: 5rpx;
}
.trade-money-icon {
	position: absolute;
	right: 40rpx;
}
.reduce-price {
	width: 150rpx;
	text-align: right;
	position: absolute;
	right: 90rpx;
}
.dh-time {
	display: inline-block;
	padding-left: 10rpx;
}
.index-tips{
	margin-top: 300rpx;
	color: #969696;
	text-align: center;
}


